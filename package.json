{
  "name": "sandbox.web",
  "version": "0.0.0",
  "description": "Sandbox for Web based on Proxy & Proto",
  "author": "nanyuantingfeng <nanyuantingfeng@aliyun.com>",
  "main": "dist/index.lib.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint src/**.ts --fix src",
    "test": "jest --coverage",
    "build": "rm -rf dist && npm run optimize",
    "optimize": "npm run build:src && npm run build:types",
    "prepublishOnly": "npm run test && npm run build",
    "build:src": "rollup -c",
    "build:types": "tsc --emitDeclarationOnly --declaration"
  },
  "license": "MIT",
  "keywords": [
    "sandbox"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "npm run lint"
    ]
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/glob": "^7",
    "@types/jest": "^27.0.2",
    "@types/node": "^16",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.5.2",
    "eslint-plugin-prettier": "^4.0.0",
    "glob": "^7",
    "husky": "^7.0.2",
    "jest": "^27.2.4",
    "lint-staged": "^11",
    "nock": "^13.1.3",
    "prettier": "^2.4.1",
    "rollup": "^2.58.0",
    "ts-jest": "^27.0.5",
    "tslib": "^2",
    "typescript": "^4",
    "xhr-mock": "^2.5.1"
  }
}
